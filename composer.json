{
  "name": "TGM/tgm-copyright",
  "type": "typo3-cms-extension",
  "description": "TYPO3 Image Copyrights and Image Sitemap",
  "homepage": "https://www.toll-paul.de",
  "license": ["GPL-2.0+"],
  "keywords": ["TYPO3 CMS", "copyright", "images", "sitemap"],
  "support": {
    "issues": "https://github.com/Schweriner/tgm_copyright/issues"
  },
  "require": {
    "typo3/cms-core": "^8.7.0 || ^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "TGM\\TgmCopyright\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/tgm-copyright": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tgm_copyright"
    }
  }
}